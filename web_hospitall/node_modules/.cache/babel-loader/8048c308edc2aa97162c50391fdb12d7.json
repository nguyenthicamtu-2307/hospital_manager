EASJ;AAhFW,CAAf","names":["Base","Table","ModalSpecicalList","Request","mapMutations","mapState","URL_IMAGE","components","computed","methods","onInput","event","loading","counter","clearTimeout","timeOut","setTimeout","Post","value","target","result","list","data","alert","setList","item","index","findIndex","dt","id","clone","editItem","setModal","modal","deleteItem","loadingColumn","Delete","status","filter","reset","urlImage","mounted","Get"],"sourceRoot":"","sources":["C:\\Users\\FPTSHOP\\Downloads\\doan-main\\doan-main\\web_hospitall\\src\\components\\manage\\pages\\SpecicalList.vue"],"sourcesContent":["<template>\n    <Base :onInput=\"onInput\">\n    <Table :loading=\"loading\" :heading=\"['Ảnh thu nhỏ', 'Tên chuyên khoa']\" :title=\"''\" :list=\"list\">\n        <tr v-for=\"(item, index) in list\" :key=\"item.id\">\n            <td>{{ index + 1 }}</td>\n            <td>\n                <img :src=\"`${urlImage + item.thumbnail}`\" style=\"width: 14rem; height:7rem;\" alt=\"\" srcset=\"\">\n            </td>\n            <td>\n                {{ item.namespecical }}\n            </td>\n            <td>\n                <span @click=\"editItem(item.id)\" class=\"bx bx-pencil\"></span>\n            </td>\n            <td>\n                <span @click=\"deleteItem(item.id)\"\n                    :class=\"`bx bx-${loadingColumn === item.id ? 'loader loading' : 'trash'}`\"></span>\n            </td>\n        </tr>\n    </Table>\n    <ModalSpecicalList v-if=\"modal.data\" :id=\"id\" :list=\"list\" :setList=\"setList\" :reset=\"reset\">\n\n    </ModalSpecicalList>\n    </Base>\n</template>\n<script>\nimport Base from \"./Base.vue\";\nimport Table from \"../Table.vue\";\nimport ModalSpecicalList from \"../modal/ModalSpecicalList.vue\";\nimport Request from \"../../../Request\";\nimport { mapMutations, mapState } from \"vuex\";\nimport { URL_IMAGE } from \"../../../Config\";\nexport default {\n    components: {\n        Base,\n        Table,\n        ModalSpecicalList\n    },\n    computed: {\n        ...mapState(['modal'])\n    },\n    methods: {\n        ...mapMutations(['setModal']),\n        onInput: function (event) {\n            this.loading = true;\n            this.counter += 1;\n            clearTimeout(this.timeOut);\n            this.timeOut = setTimeout(async () => {\n                try {\n                    const result = await Request.Post(`/specicallist-search`, {\n                        value: event.target.value\n                    });\n                    this.list = result.data.data;\n                    this.loading = false;\n                } catch (error) {\n                    alert(error);\n                }\n            }, 300);\n        },\n        setList: function (item) {\n            const index = [...this.list].findIndex(dt => dt.id === item.id);\n            if (index === -1) {\n                this.list = [...this.list, item];\n            }\n            else {\n                let clone = [...this.list];\n                clone[index] = item;\n                this.list = clone;\n            }\n        },\n        editItem: function (id) {\n            this.id = id;\n            this.setModal({ ...this.modal, data: true });\n        },\n        deleteItem: async function (id) {\n            try {\n                this.loadingColumn = id;\n                const result = await Request.Delete(`/specicallists/${id}`);\n                if (result.data.status) {\n                    this.list = [...this.list].filter(dt => dt.id !== id);\n                }\n                this.loadingColumn = -1;\n            } catch (error) {\n                alert(error);\n            }\n        },\n        reset: function () {\n            this.id = null;\n            this.data = null;\n        }\n    },\n    data() {\n        return {\n            list: [],\n            loading: true,\n            id: null,\n            urlImage: URL_IMAGE,\n            loadingColumn: -1,\n            timeOut: null,\n            counter: 0\n        }\n    },\n    mounted() {\n        (async () => {\n            try {\n                const result = await Request.Get(\"/specicallists\");\n                this.list = result.data.data;\n                this.loading = false;\n            } catch (error) {\n                alert(error)\n            }\n        })();\n    }\n}\n</script>"]}]}